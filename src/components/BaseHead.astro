---
import { ClientRouter } from "astro:transitions";
import senWoff2 from "@fontsource/sen/files/sen-latin-400-normal.woff2?url";

import "@fontsource/sen";
import "@fontsource/sen/500.css";
import "@fontsource/sen/600.css";
import "@fontsource/sen/700.css";
---

<!-- Global Metadata -->
<meta charset="UTF-8" />
<meta name="viewport" content="width=device-width,initial-scale=1" />
<meta name="generator" content={Astro.generator} />
<link rel="sitemap" href="/sitemap-index.xml" />

<!-- Favicons -->
<link rel="icon" type="image/png" href="/favicon-96x96.png" sizes="96x96" />
<link rel="icon" type="image/svg+xml" href="/favicon.svg" />
<link rel="shortcut icon" href="/favicon.ico" />
<link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
<meta name="apple-mobile-web-app-title" content="Felipe Buencamino" />
<link rel="manifest" href="/site.webmanifest" />

<!-- Fonts -->
<link
  rel="preload"
  as="font"
  type="font/woff2"
  href={senWoff2}
  crossorigin="anonymous"
/>

<!-- Analytics -->
<script
  is:inline
  defer
  type="text/partytown"
  src="https://cloud.umami.is/script.js"
  data-website-id="a4a4dbfb-1d2f-49f5-8f4c-4b6da60f5a3d"
  data-domains="felipe-buencamino.onrender.com,www.felipe-buencamino.onrender.com,felipebuencamino.com,www.felipebuencamino.com"
></script>

<ClientRouter />
